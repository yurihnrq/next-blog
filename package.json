{
  "name": "next-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "prepare": "husky install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.4",
    "@types/node": "17.0.18",
    "@types/react": "17.0.39",
    "@types/styled-components": "^5.1.23",
    "eslint": "8.9.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "prettier": "2.6.1",
    "typescript": "4.5.5"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx,css}": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css}'",
    "**/*.{ts,tsx,js,jsx}": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'"
  }
}
